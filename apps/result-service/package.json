{
  "name": "@xingu/result-service",
  "version": "1.0.0",
  "private": true,
  "description": "Game results and statistics service",
  "scripts": {
    "build": "pnpm exec tsc",
    "dev": "dotenv -e ../../.env -- tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "pnpm exec tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@sentry/node": "^10.26.0",
    "@sentry/profiling-node": "^10.26.0",
    "@xingu/database": "workspace:*",
    "@xingu/shared": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
